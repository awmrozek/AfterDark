<Window x:Class="AfterDarkness.MainWindow"
        xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
        xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
        xmlns:d="http://schemas.microsoft.com/expression/blend/2008"
        xmlns:mc="http://schemas.openxmlformats.org/markup-compatibility/2006"
        xmlns:local="clr-namespace:AfterDarkness"
        mc:Ignorable="d"
        Title="MainWindow" Height="450" Width="800">
    <Grid>
        <Button x:Name="LightModeButton" Content="Set Light Mode" HorizontalAlignment="Left" Height="70" Margin="41,35,0,0" VerticalAlignment="Top" Width="197" Click="Button_Click"/>
        <Button x:Name="DarkModeButton" Content="Set Dark Mode" HorizontalAlignment="Left" Height="80" Margin="41,137,0,0" VerticalAlignment="Top" Width="197" Click="DarkModeButton_Click"/>

    </Grid>
</Window>
